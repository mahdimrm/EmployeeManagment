@using Employee.Entities;
@model Grade
@{
    ViewData["Title"] = "ایجاد درجه";
    Layout = null;
}
<div class="col-md-12">
    <form asp-action="Create" asp-controller="Grade" method="post">
        <div class="form-group">
            <label asp-for="Name" class="control-label"></label>
            <input asp-for="Name" required type="text" class="form-control" />
        </div>

        <div class="form-group">
            <button class="btn btn-primary btn-block">افزودن</button>
        </div>
    </form>
</div>

